{
    "name": "endroid/flusher",
    "description": "Endroid Flusher",
    "keywords": ["endroid", "flusher", "php"],
    "homepage": "https://github.com/endroid/flusher",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl"
        }
    ],
    "require": {
        "php": "^8.4",
        "doctrine/orm": "^3.3",
        "endroid/installer": "^1.5",
        "symfony/cache": "^6.4||^7.4||^8.0",
        "symfony/stopwatch": "^6.4||^7.4||^8.0"
    },
    "require-dev": {
        "endroid/quality": "dev-main"
    },
    "suggest": {
        "roave/security-advisories": "Avoids installation of package versions with vulnerabilities"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\Flusher\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\Flusher\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "endroid/installer": true
        }
    }
}
